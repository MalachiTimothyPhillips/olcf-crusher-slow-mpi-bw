[GENERAL]
polynomialOrder = 7
stopAt = endTime
endTime = 100
dt = 2e-02
timeStepper = tombo2
subcyclingSteps = 2

writeControl = runTime
writeInterval = 10

[PROBLEMTYPE]
stressFormulation = true # required for variable viscosity

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
