[GENERAL]
polynomialOrder = 7
stopAt = numSteps
numSteps = 100
dt = 1e-3
timeStepper = tombo3
writeInterval = 50

[MESH]
boundaryTypeMap = fixedValue, zeroValue, symX
solver = elasticity

[PROBLEMTYPE]
stressFormulation = true

[PRESSURE]
residualTol = 1e-8
initialGuess = none

[VELOCITY]
boundaryTypeMap = fixedValue, zeroValue, symX
residualTol = 1e-12

[TEMPERATURE]
boundaryTypeMap = zeroGradient,zeroGradient,zeroGradient
residualTol = 1e-12