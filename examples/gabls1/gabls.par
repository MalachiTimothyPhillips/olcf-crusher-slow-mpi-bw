[GENERAL]
#verbose = true
polynomialOrder = 9
#startFrom = "restart.fld"
stopAt = endTime
endTime = 2600

dt = 1.0e-2
timeStepper = tombo2
subCyclingSteps = 1

writeControl = runTime
writeInterval = 100

regularization = hpfrt + nModes = 1 + scalingCoeff = 0.05/${dt}

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = wall, inlet
residualTol = 1e-06
density = 1.0
viscosity = -50e3

[TEMPERATURE]
boundaryTypeMap = inlet, flux 
residualTol = 1e-06
rhoCp = 1.0
conductivity = -50e3
