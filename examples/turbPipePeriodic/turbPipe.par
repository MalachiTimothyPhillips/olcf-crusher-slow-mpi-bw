[GENERAL]
polynomialOrder = 7
#startFrom = "restart.fld"
stopAt = endTime
endTime = 200

dt = 6.0e-3
timeStepper = tombo2
extrapolation = subCycling
subCyclingSteps = 2

writeControl = runTime
writeInterval = 20

regularization = hpfrt + nModes = 1 + scalingCoeff = 0.1/${dt}

# constant flow rate driver
constFlowRate = meanVelocity=1.0 + direction=Z

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = wall
viscosity = -19000
density = 1.0
residualTol = 1e-06
