[GENERAL] 
polynomialOrder = 7
#startFrom = "restart.fld"
numSteps = 100
dt = 1e-03
timeStepper = tombo2
writeInterval = 0

usr = "channel.usr"
oudf = "channel.oudf"
udf = "channel.udf"

[MESH]
file = "channel.re2"

[PROBLEMTYPE]
equation = Stokes
stressFormulation = true # required for variable viscosity

[PRESSURE]
residualTol = 1e-08

[VELOCITY]
boundaryTypeMap = shl
residualTol = 1e-12
density = 1.0
viscosity = 0.1 

[CASEDATA]
p_rot = 0.7853981633974483 # \pi / 4
